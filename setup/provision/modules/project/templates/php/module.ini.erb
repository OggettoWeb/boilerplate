; NOTICE : File auto-generated by puppet, do not edit
<% if @auto_enable -%>
; Enable <%= @modname %> extension module
<% if @zend -%>
zend_extension=<%= @zend %>/<%= @modname %>.so
<% else -%>
extension=<%= @modname %>.so
<% end -%>
<% end -%>

<% if !@settings.empty? -%>
; Custom settings
<% @settings.sort_by {|key, value| key}.each do |key,value| -%>
<%= key %>=<%= value %>
<% end -%>

<% end -%>
